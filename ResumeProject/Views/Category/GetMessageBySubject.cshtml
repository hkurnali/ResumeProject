
@using ResumeProject.Models
@model List<TBLCONTACT>
@{
    ViewBag.Title = "GetMessageBySubject";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Gelen Mesajlar</h2>


<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Ad Soyad</th>
        <th>Konu</th>
        <th>Tarih</th>
        <th>Sil</th>
      
        <th>Mesajı Aç</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.ContactID</td>
            <td>@item.NameSurname</td>
            <td>@item.TBLCATEGORY.CategoryName</td>
            <td>@item.Date</td>
            <td><a href="/Contact/DeleteContact/@item.ContactID" class="btn btn-outline-danger">Sil</a></td>
            
            <td><a href="#" class="btn btn-outline-primary">Mesajı Aç</a></td>

        </tr>
    }


</table>
