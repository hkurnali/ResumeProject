
<footer class="text-center">
    <div class="footer py-5">
        <div class="container py-md-4">
            <div clss="footer-logo">
                <a class="logo" href="index.html">
                    <i class="fab fa-asymmetrik"></i> Hüseyin Kurnalı
                </a>
            </div>
            <div class="footer-contact-info mt-4">
                
                <ul>
                    <li>
                        <a href="tel:+12 345 678 900" class="contact-text-sub">
                            <span class="fas fa-phone me-2"></span>@ViewBag.phoneFooter
                        </a>
                    </li>
                    <li>
                        <a href="mailto:info@example.com" class="contact-text-sub">
                            <span class="fas fa-envelope me-2"></span>@ViewBag.emailFooter
                        </a>
                    </li>
                    <li>
                        <p class="contact-text-sub">
                            <span class="fas fa-map-marker me-2"></span>@ViewBag.addressFooter
