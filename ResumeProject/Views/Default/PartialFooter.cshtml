                    <ul>
                        <li><a href="@ViewBag.smadia1Footer"><span class="fab me-2 fa-facebook-f"></span></a></li>
                        <li><a href="@ViewBag.smadia2Footer"><span class="fab me-2 fa-twitter"></span></a></li>
